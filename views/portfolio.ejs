    <script>
      const storageKey = 'formDataList';
      

      function storeFormData(formId, descId, imgId, qtyId, priceId) {
        
            const data = {
                formId: formId,
                description: document.getElementById(descId).textContent,
                imageSrc: document.getElementById(imgId).src,
                quantity: document.getElementById(qtyId).textContent,
                price: document.getElementById(priceId).textContent,
                
            };

            let formDataList = JSON.parse(localStorage.getItem(storageKey)) || [];
            const formIndex = formDataList.findIndex(item => item.formId === formId);

            if (formIndex !== -1) {
                // Update existing form data
                formDataList[formIndex] = data;
            } else {
                // Add new form data
                formDataList.push(data);
            }

            localStorage.setItem(storageKey, JSON.stringify(formDataList));
            updateItemCount();
        }

        function displayStoredData() {
            const formDataList = JSON.parse(localStorage.getItem(storageKey)) || [];
            const displayDiv = document.getElementById('displayDiv');            
            displayDiv.innerHTML = ''; // Clear previous content

            if (formDataList.length === 0) {
                displayDiv.textContent = 'No Item available';
            } else {
                const ul = document.createElement('ul');
                ul.style.paddingLeft = '0px';
                formDataList.forEach((data, index) => {
                    const li = document.createElement('li');
                    li.style.display = 'flex';
                    li.style.justifyContent = "center";
                    li.style.alignItems = "center";
                    li.style.marginBottom = '4px';
                    const img = document.createElement('img');
                    img.src = data.imageSrc;
                    img.alt = 'Stored Image';
                    img.style.width = '150px'; // Set fixed width
                    img.style.height = '80px'; // Maintain aspect ratio

                    

                    // li.textContent = data.description;
                    // li.appendChild(document.createElement('br')); 
                    li.appendChild(img);                    

                    const itemName = document.createElement('span');
                    itemName.style.width = '150px';
                    // itemName.style.height = '80px';
                    // itemName.style.marginBottom = '4px';
                    itemName.style.borderColor = "red";
                    itemName.style.borderLeftWidth = '10px';
                    itemName.textContent = data.description;
                    li.appendChild(itemName);

                    const qtyInfo = document.createElement('span');
                    qtyInfo.style.width = '150px';
                    qtyInfo.textContent = `Qty: ${data.quantity}`;              
                    li.appendChild(qtyInfo);

                    const priceInfo = document.createElement('span');
                    priceInfo.style.width = '150px';
                    priceInfo.textContent = data.price;
                    li.appendChild(priceInfo);

                    const removeBtn = document.createElement('span');
                    removeBtn.textContent = 'Del';
                    removeBtn.style.backgroundColor = '#AA0000';
                    removeBtn.style.color = 'white';
                    removeBtn.style.padding = '4px';
                    removeBtn.style.borderRadius = '10px';
                    removeBtn.style.height = '30px';
                    removeBtn.style.cursor = 'pointer';
                    removeBtn.classList.add('remove-btn');
                    removeBtn.onclick = () => removeFormData(index);
                    li.appendChild(removeBtn);

                    li.appendChild(document.createElement('hr')); 

                    ul.appendChild(li);
                });
                displayDiv.appendChild(ul);
            }

            displayDiv.style.display = 'block'; // Show the display area
        }

        function removeFormData(index) {
            let formDataList = JSON.parse(localStorage.getItem(storageKey)) || [];
            formDataList.splice(index, 1); // Remove the item at the given index
            localStorage.setItem(storageKey, JSON.stringify(formDataList));
            displayStoredData(); // Refresh the display
        }

        // Optionally, you can clear the stored data
        function clearStoredData() {
            localStorage.removeItem(storageKey);
            const displayDiv = document.getElementById('displayDiv');
            displayDiv.innerHTML = ''; // Clear display content
            displayDiv.style.display = 'none'; // Hide the display area
        }
        
        const itemCountDiv = document.getElementById("itemCountDiv");

        function updateItemCount() {
            const formDataList = JSON.parse(localStorage.getItem(storageKey)) || [];
            itemCountDiv.textContent = formDataList.length;
        }

        updateItemCount();
    </script>
    <script>
      function sendEmail() {
          const contentDiv = document.getElementById('displayDiv').innerHTML;
          const email = 'everythingvodas@gmail.com'; // replace with the recipient email

          // http://localhost:3000/send-email
          // https://vodasrental.onrender.com/portfolio/send-email
          fetch('http://localhost:4014/send-email', {
              method: 'POST',
              headers: {
                  'Content-Type': 'application/json'
              },
              body: JSON.stringify({
                  email: email,
                  content: contentDiv
              })
          })
          .then(response => {
              if (!response.ok) {
                  throw new Error('Network response was not ok ' + response.statusText);
              }
              return response.text();
          })
          .then(data => {
            window.location.href = "http://localhost:4014/venue";
          })
          .catch((error) => {
              console.error('Error:', error);
              alert('Error sending email: ' + error);
          });
      }
